swapWo3V.php
<?php
$A=$_GET['a'];
$B=$_GET['b'];
	echo "Value of A:".$A."<br>";
	echo "Value of B:".$B."<br>";
	
	echo "Values after SWAPPING are:"."<br>";
	
		$A=$A+$B;
		$B=$A-$B;
		$A=$A-$B;

echo "Value of A:".$A."<br>";
echo "Value of B:".$B."<br>";
?>
